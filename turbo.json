{
    "$schema": "https://turborepo.com/schema.json",
    "ui": "stream",
    "tasks": {
        "dev": {
            "cache": false,
            "persistent": true,
            "inputs": ["$TURBO_DEFAULT$", ".env"]
        },
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULT$", ".env"]
        },
        "start": {
            "dependsOn": ["^start"]
        },
        "lint": {
            "dependsOn": ["^lint"]
        }
    }
}
